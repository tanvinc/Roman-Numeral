{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "space-before-function-paren": ["error", {
            "named": "ignore",
            "anonymous": "always"    
        }],
        "strict": ["error", "global"]
    },
    "env":{
        "node": true,
        "es6": true
    } 
}